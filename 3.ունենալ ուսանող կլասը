=======================================================================================Usanox class
public class Usaxox {
   private String name;
   private String lastName;
   private int age;
   private int rating;

   public Usaxox() {
   }
 public Usaxox(String name, String lastName, int age, int rating) {
      this.name = name;
      this.lastName = lastName;
      this.age = age;
      this.rating = rating;


   }
   public String getName() {
      return name;
   }

   public void setName(String name) {
      this.name = name;
   }

   public String getLastName() {
      return lastName;
   }

   public void setLastName(String lastName) {
      this.lastName = lastName;
   }

   public int getAge() {
      return age;
   }

   public void setAge(int age) {
      this.age = age;
   }

   public int getRating() {
      return rating;
   }

   public void setRating(int rating) {
      this.rating = rating;
   }


}
